{
  "name": "school-tutor-evaluation",
  "version": "1.0.0",
  "description": "Comprehensive evaluation framework for School Tutor Agent using Ragas, DeepEval, TruLens, and custom metrics",
  "main": "index.js",
  "scripts": {
    "install:all": "pnpm install && pip install -r requirements.txt",
    "eval:ragas": "python frameworks/ragas/evaluate.py",
    "eval:deepeval": "python frameworks/deepeval/evaluate.py", 
    "eval:trulens": "python frameworks/trulens/evaluate.py",
    "eval:custom": "python frameworks/custom/evaluate.py",
    "eval:all": "pnpm run eval:ragas && pnpm run eval:deepeval && pnpm run eval:trulens && pnpm run eval:custom",
    "dashboard": "python dashboard/app.py",
    "report": "python scripts/generate_report.py --output ../dist/evaluation/reports",
    "benchmark": "python scripts/benchmark.py --output ../dist/evaluation/benchmarks",
    "test": "python -m pytest tests/",
    "lint": "black . && flake8 .",
    "type-check": "mypy ."
  },
  "keywords": [
    "evaluation",
    "llm",
    "ragas",
    "deepeval",
    "trulens",
    "ai-tutor",
    "education"
  ],
  "author": "Pankaj Negi",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  }
}

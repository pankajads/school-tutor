{
  "name": "school-tutor-backend",
  "version": "3.0.0",
  "description": "AWS CDK backend for school tutor agent with Bedrock integration",
  "bin": {
    "school-tutor-backend": "bin/school-tutor-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "pnpm exec cdk doctor",
    "deploy": "pnpm run build && pnpm exec cdk deploy CoreResourcesStack",
    "deploy:all": "pnpm run build && pnpm exec cdk deploy --all",
    "deploy:ai": "pnpm run build && pnpm exec cdk deploy AiServiceStack",
    "destroy": "pnpm exec cdk destroy --all",
    "synth": "pnpm exec cdk synth",
    "diff": "pnpm exec cdk diff",
    "bootstrap": "pnpm exec cdk bootstrap",
    "start": "pnpm run build && node ../dist/backend/src/cli/index.js",
    "cli": "pnpm run build && node ../dist/backend/src/cli/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.14.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "aws-cdk": "^2.1026.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@aws-cdk/aws-bedrock-alpha": "^2.212.0-alpha.0",
    "@aws-sdk/client-bedrock": "^3.873.0",
    "@aws-sdk/client-bedrock-agent": "^3.873.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.873.0",
    "@aws-sdk/client-bedrock-runtime": "^3.873.0",
    "@aws-sdk/client-cloudwatch": "^3.873.0",
    "@aws-sdk/client-dynamodb": "^3.873.0",
    "@aws-sdk/client-lambda": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/lib-dynamodb": "^3.873.0",
    "@aws-sdk/util-dynamodb": "^3.873.0",
    "aws-cdk-lib": "^2.212.0",
    "commander": "^11.1.0",
    "constructs": "^10.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}

{
  "name": "school-tutor-backend",
  "version": "1.0.0",
  "description": "AWS CDK backend for school tutor agent with Bedrock integration",
  "bin": {
    "school-tutor-backend": "bin/school-tutor-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "pnpm run build && cdk deploy --all",
    "deploy:dev": "pnpm run build && cdk deploy --all --profile dev",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "start": "pnpm run build && node ../dist/backend/cli/index.js",
    "cli": "pnpm run build && node ../dist/backend/cli/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-bedrock-alpha": "^2.117.0-alpha.0",
    "@aws-sdk/client-bedrock": "^3.490.0",
    "@aws-sdk/client-bedrock-agent": "^3.490.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.490.0",
    "@aws-sdk/client-bedrock-runtime": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "aws-cdk-lib": "^2.117.0",
    "commander": "^11.1.0",
    "constructs": "^10.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
